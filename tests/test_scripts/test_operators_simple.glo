# Simple test for C++ style operators
# Testing increment operators

x = 5
print "x =", x
x++
print "x++ result: x =", x

y = 10
print "y =", y
++y
print "++y result: y =", y

# Testing compound assignment
z = 20
print "z =", z
z += 5
print "z += 5 result: z =", z
z -= 3
print "z -= 3 result: z =", z
z *= 2
print "z *= 2 result: z =", z
z /= 4
print "z /= 4 result: z =", z

print "All tests completed!"
